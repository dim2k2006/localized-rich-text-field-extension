    "lodash": "^4.17.21",
    "phosphor-react": "^1.4.1",
    "prop-types": "15.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "browserslist": [
    "last 5 Chrome version",
    "> 1%",
    "not ie <= 11"
  ]
